<h1><%= @course.name %></h1>
<%= render :partial => 'admin/courses/menu', :locals => {:course => @course} %>
<article>
  <header>
    <h2><%= t 'Add' %> <%= t 'Student' %></h2>
    <ul id="toolbar">
      <li><%= link_to (t 'Back'), admin_course_course_students_path(@course), :class => 'back' %></li>
    </ul>
  </header>

  <%= render 'form' %>
</article>

<script language="JavaScript">
  $(document).ready(function() {
    autocomplete('student_code', '<%= admin_students_path %>.js', '<%= t "Remove" %>')
  });
</script>
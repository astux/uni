<h1><%= @course.name %></h1>
<%= render :partial => 'admin/courses/menu', :locals => { :course => @course } %>
<article>
  <header>
    <h2><%= t 'Editing' %> <%= t 'CurriculumDiscipline' %></h2>
    <ul id="toolbar">
      <li><%= link_to  (t 'Show'), [:admin, @course, @discipline], :class => 'show' %></li>
      <li><%= button_to (t 'Delete'), [:admin, @course, @discipline], :confirm => 'Tem certeza disso?', :method => :delete, :class => 'delete' %></li> 
      <li><%= link_to (t 'Back'), admin_course_disciplines_path(@course), :class => 'back' %></li>
    </ul>
  </header>
  
  <%= render 'form' %>
</article>

<table class="grid">
  <thead>
    <tr>
    	<% for key, value in @tests_and_results %>
      <th>
      	<%= value[:test].name %> 
      	<span class="small-relevance">(<%= l value[:test].scheduled_date, :format => :short %>)</span>
     	</th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <tr>
    	<% for key, value in @tests_and_results %>
      <td><%= value[:result].rating if value[:result] %></td>
      <% end %>
    </tr>
  </tbody>
</table>